   "source": [
    "# Import packages here"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "import nltk\n",
    "from bertopic import BERTopic\n",
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "import pandas as pd\n",
    "# from cuml.cluster import HDBSCAN # We are using the GPU accelerated version of HDBSCAN\n",
    "# from cuml.manifold import UMAP # We are using the GPU accelerated version of UMAP\n",
    "from umap import UMAP # CPU based calculation\n",
    "from hdbscan import HDBSCAN # CPU based calculation\n",
    "from sklearn.cluster import KMeans\n",
    "# from cuml.cluster import KMeans\n",
    "import numpy as np\n",
    "import openai\n",
    "from bertopic.backend import OpenAIBackend\n",
    "from tqdm import tqdm\n",
    "from bertopic.representation import OpenAI\n",
    "from octis.evaluation_metrics.diversity_metrics import TopicDiversity\n",
    "import gensim.corpora as corpora\n",
    "from gensim.models.coherencemodel import CoherenceModel"
   ]
  },
  {
   "attachments": {},
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Load data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_staff_scheduling_whole = pd.read_pickle(f'/mnt/d/Denis/Daten 2.0/Studium/WS23/Masterseminararbeit_Bertopic_analysis/preprocessed_data/prep_wos.pkl')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_final = df_staff_scheduling_whole[df_staff_scheduling_whole['forward_from'] != 'jouini_et_al_2008'].reset_index(drop=True)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Obtain Open AI embeddings"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 59,
   "metadata": {},
   "outputs": [],
   "source": [
    "MY_API_KEY = "",
    "openai.api_key = MY_API_KEY"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 60,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_embedding(text, model='text-embedding-ada-002'):\n",
    "    return openai.Embedding.create(input = [text], model=model)['data'][0]['embedding']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_final['ada_embedding'] = df_final['Tokenized_Abstracts'].apply(lambda x: get_embedding(x, model=\"text-embedding-ada-002\"))\n",
    "df_final.to_csv('embedded_final_tokenized_abstracts.csv', encoding='UTF-8', index=False)\n",
    "df_final.to_pickle('embedded_final_tokenized_abstracts.pkl')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Loading embedding data whole"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_token = pd.read_csv('embedded_final_tokenized_abstracts.csv')\n",
    "df_token['ada_embedding'] = df_token.ada_embedding.apply(eval).apply(np.array)"
   ]
  },
  {
   "attachments": {},
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Training Literature"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "docs = list(df_final['Abstract'])\n",
    "embeddings = np.vstack(df_token['ada_embedding'])\n",
    "titles = list(df_final['Article Title'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "embeddings"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# CountVectorizer and NLTK Stopwords"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "[nltk_data] Downloading package stopwords to /home/denis/nltk_data...\n",
      "[nltk_data]   Package stopwords is already up-to-date!\n"
     ]
    }
   ],
   "source": [
    "nltk.download('stopwords')\n",
    "vectorizer_model = CountVectorizer(\n",
    "    stop_words=nltk.corpus.stopwords.words('english'),\n",
    "    ngram_range=(1,2),\n",
    "    min_df=2\n",
    "    )"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# OpenAI key"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "MY_API_KEY = \"XXXX\"\n",
    "openai.api_key = MY_API_KEY"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# KeyBertInspired Representation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "from bertopic.representation import KeyBERTInspired\n",
    "from bertopic import BERTopic\n",
    "\n",
    "# Create your representation model\n",
    "keybert_representation = KeyBERTInspired()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Training with KMeans"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "metadata": {},
   "outputs": [],
   "source": [
    "kmeans = KMeans(n_clusters = 20, init='k-means++', random_state=42)\n",
    "umap_model = UMAP(n_neighbors=5, n_components=270, min_dist=0.0, metric='cosine', verbose=False, random_state=42, n_epochs=2000)\n",
    "\n",
    "topic_model = BERTopic(\n",
    "    umap_model=umap_model,\n",
    "    hdbscan_model=kmeans,\n",
    "    vectorizer_model=vectorizer_model,\n",
    "    representation_model=keybert_representation,\n",
    "    embedding_model = OpenAIBackend(embedding_model='text-embedding-ada-002', batch_size=200),\n",
    "                    )\n",
    "topics, probs = topic_model.fit_transform(docs, embeddings)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Training with HDBSCAN"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "umap_model = UMAP(n_neighbors=5, n_components=270, min_dist=0.0, metric='cosine', verbose=False, random_state=42, n_epochs=2000)\n",
    "hdbscan_model = HDBSCAN(min_cluster_size=10, metric='euclidean', cluster_selection_method='leaf', prediction_data=True, )\n",
    "\n",
    "topic_model = BERTopic(\n",
    "    umap_model=umap_model,\n",
    "    hdbscan_model=hdbscan_model,\n",
    "    vectorizer_model=vectorizer_model,\n",
    "    representation_model=keybert_representation,\n",
    "    embedding_model = OpenAIBackend(embedding_model='text-embedding-ada-002', batch_size=200),\n",
    "                    )\n",
    "topics, probs = topic_model.fit_transform(docs, embeddings)"
   ]
  },
